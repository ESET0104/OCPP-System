{
  "openapi": "3.1.1",
  "info": {
    "title": "BackendAPI | v1",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://localhost:7127"
    },
    {
      "url": "http://localhost:5161"
    }
  ],
  "paths": {
    "/api/chargers": {
      "get": {
        "tags": [
          "Charger"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/chargers/Add": {
      "post": {
        "tags": [
          "Charger"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/sessions": {
      "get": {
        "tags": [
          "Session"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/sessions/start": {
      "post": {
        "tags": [
          "Session"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/StartSessionReq"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/StartSessionReq"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/StartSessionReq"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/sessions/stop": {
      "post": {
        "tags": [
          "Session"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/StopSessionReq"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/StopSessionReq"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/StopSessionReq"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "StartSessionReq": {
        "type": "object",
        "properties": {
          "chargerId": {
            "type": "string"
          },
          "userId": {
            "type": "string"
          }
        }
      },
      "StopSessionReq": {
        "type": "object",
        "properties": {
          "sessionId": {
            "type": "string"
          }
        }
      }
    }
  },
  "tags": [
    {
      "name": "Charger"
    },
    {
      "name": "Session"
    }
  ]
}